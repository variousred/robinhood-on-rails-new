.col-xs-10{data:{behavior: "FundamentalsTooltip"}}
  %h2
    Dividends
    %small= (price_display @dividends.map{|d| d["amount"].to_f }.sum) + " total"
  - @dividends.each do |dividend|
    .col-xs-4.col-sm-3.fundamentals-tooltip
      %h3= price_display dividend["amount"]
      = stock_link dividend["instrument"]["symbol"], {class: "symbol", data: {html: true, placement: :right}}
      %br
      Expected
      = dividend["payable_date"]
      %br
      = price_display dividend["rate"]
      per share
      %br
      = dividend["position"].to_i
      shares owned

  = javascript_include_tag "https://www.gstatic.com/charts/loader.js"